	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

		<Button
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Toggle headband"
			android:id="@+id/useHeadband"
			android:layout_gravity="bottom" />

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textAppearance="?android:attr/textAppearanceLarge"
			android:text="Large Text"
			android:id="@+id/textViewStatus"
			android:layout_gravity="center_vertical" />

		<Button
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Up"
			android:id="@+id/upButton" />

	</LinearLayout>

	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">

		<LinearLayout
			android:orientation="vertical"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2">

			<Button
				android:id="@+id/pause"
				android:layout_width="150dp"
				android:layout_height="40dp"
				android:text="@string/pause" />

			<LinearLayout
				android:orientation="horizontal"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent">

				<Button
					android:layout_width="0dp"
					android:layout_height="40dp"
					android:text="TakeOff"
					android:id="@+id/takeoffBt"
					android:layout_weight="1" />

				<Button
					android:layout_width="0dp"
					android:layout_height="40dp"
					android:text="Land"
					android:id="@+id/landingBt"
					android:layout_weight="1" />
			</LinearLayout>

			<Button
				android:layout_width="fill_parent"
				android:layout_height="40dp"
				android:text="Emergency"
				android:id="@+id/emergencyBt" />
		</LinearLayout>

		<Button
			style="?android:attr/buttonStyleSmall"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:text="Left"
			android:id="@+id/rollLeftBt"
			android:layout_weight="1"
			android:layout_gravity="center_vertical" />

		<LinearLayout
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:layout_weight="1"
			android:layout_gravity="center_vertical">

			<Button
				style="?android:attr/buttonStyleSmall"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text="Forward"
				android:id="@+id/forwardBt" />

			<Button
				style="?android:attr/buttonStyleSmall"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:text="Back"
				android:id="@+id/backBt" />
		</LinearLayout>

		<Button
			style="?android:attr/buttonStyleSmall"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:text="Right"
			android:id="@+id/rollRightBt"
			android:layout_weight="1"
			android:layout_gravity="center_vertical" />

	</LinearLayout>

